# SQL Server to Snowflake Query Performance Comparison Tool
# Environment Variables Configuration Template
#
# Copy this file to .env and fill in your actual credentials
# The .env file is gitignored and should never be committed

# ============================================
# SQL Server Test Run Database Connection
# ============================================
SQL_SERVER_TEST_RUN_SERVER=localhost
SQL_SERVER_TEST_RUN_DATABASE=TestRunDB
SQL_SERVER_TEST_RUN_PORT=1433
SQL_SERVER_TEST_RUN_USER=your_username
SQL_SERVER_TEST_RUN_PASSWORD=your_password

# ============================================
# SQL Server Test Target Database Connection
# ============================================
SQL_SERVER_TEST_TARGET_SERVER=localhost
SQL_SERVER_TEST_TARGET_DATABASE=TestTargetDB
SQL_SERVER_TEST_TARGET_PORT=1433
SQL_SERVER_TEST_TARGET_USER=your_username
SQL_SERVER_TEST_TARGET_PASSWORD=your_password

# ============================================
# Snowflake Connection
# ============================================
SNOWFLAKE_ACCOUNT=your_account_identifier
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=TEST_DB
SNOWFLAKE_SCHEMA=PUBLIC
SNOWFLAKE_ROLE=ACCOUNTADMIN

# ============================================
# Execution Settings (Optional)
# ============================================
# PARALLEL_WORKERS=10
# REPEAT_COUNT=5
